<ol>
{% for post in postslist | reverse %}
  <li class="postlist-item{% if post.url == url %} postlist-item-active{% endif %}">
    <small><time class="mr-8" datetime="{{ post.date | htmlDateString }}">{{ post.date | htmlDateString }}</time></small>
    <a href="{{ post.url | url }}" class="">{% if post.data.title %}{{ post.data.title }}{% endif %}</a>
  </li>
{% endfor %}
</ol>